package src.events {		import flash.events.Event;		public class VideoControllerEvent extends Event {				public static const PLAY_CLICKED = "play_clicked";		public static const MUTE_CLICKED = "mute";		public static const SHARE_CLICKED = "share_clicked";		public static const FULL_CLICKED = "full_clicked";		public static const CHANGE_SOURCE = "change_source";				public static const VIDEO_SCRUB = "video_scrub";		public static const VIDEO_SCRUBBED = "video_scrubbed";		private var _scrubPercent:Number;				public function VideoControllerEvent(type:String, num:Number=0):void {						_scrubPercent = num;						super (type);		}				public function get scrubPercent():Number {			return _scrubPercent;		}	}}