package src.events {		import flash.events.Event;		public class VideoScrubEvent extends Event {				public static const VIDEO_SCRUB = "video_scrub";		public static const VIDEO_SCRUBBED = "video_scrubbed";				private var _scrub_percent:Number;				public function VideoScrubEvent(type:String, percent:Number=0):void {						_scrub_percent = percent;						super (type);					}				public function get scrubPercent():Number {			return _scrub_percent;		}	}}